###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/isRemoved
ieso:isRemoved
  rdf:type
    owl:DatatypeProperty ,
    owl:FunctionalProperty ;
  rdfs:comment "The 'is removed' datatype property relates a market 'Bid' or 'Player' with its 'is removed' property.\n\nIt states if a 'Bid' or 'Player' is removed from a market pool (period) or session and if true, a 'removal justification' property must also be attached to the respective individual ('Bid' or 'Player')."@en ;
  rdfs:label "is removed"@en ;
  rdfs:isDefinedBy <trading/v1.0.0/> ;
  rdfs:domain [
    rdf:type owl:Class ;
    owl:unionOf (
      ieso:BidResult
      ieso:PlayerResult
    )
  ] ;
  rdfs:range xsd:boolean ;
  .
# !



  rdfs:label "Auction-based market"@en ;
  rdfs:isDefinedBy <trading/v1.0.0/> ;
  rdfs:subClassOf
    ieso:MarketType ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:tradingPool ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange [
        rdf:type rdfs:Datatype ;
        owl:oneOf [
          rdf:type rdf:List ;
          rdf:first "asymmetric pool" ;
          rdf:rest [
            rdf:type rdf:List ;
            rdf:first "symmetric pool" ;
            rdf:rest rdf:nil
          ]
        ]
      ]
    ] ;

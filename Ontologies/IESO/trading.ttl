###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Bid
ieso:Bid
  rdf:type owl:Class ;
  rdfs:comment "In electricity markets, a Bid is composed of (at least) an amount of power supply or demand and a price offer to pay or receive for the supply or demand, respectively."@en ;
  rdfs:label "Bid"@en ;
  rdfs:isDefinedBy <trading/v1.0.0/> ;
  rdfs:seeAlso <http://www.mascem.gecad.isep.ipp.pt/ontologies/electricity-markets.owl#Bid> ;
  rdfs:subClassOf
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:bidId ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:unsignedInt
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasOffer ;
      owl:someValuesFrom ieso:Offer
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:placedByPlayer ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ieso:Player
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:transactionType ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange [
        rdf:type rdfs:Datatype ;
        owl:oneOf [
          rdf:type rdf:List ;
          rdf:first "buy" ;
          rdf:rest [
            rdf:type rdf:List ;
            rdf:first "sell" ;
            rdf:rest rdf:nil
          ]
        ]
      ]
    ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/BlockOrder
ieso:BlockOrder
  rdf:type owl:Class ;
  rdfs:comment "The 'Block order' class describes block bids.\n\nThis type of bid spans over a number of periods, unlike a regular order or simple bid. A block order consists of a specified volume and price for a certain number of consecutive hours within the same day."@en ;
  rdfs:label "Block order"@en ;
  rdfs:isDefinedBy <trading/v1.0.0/> ;
  rdfs:seeAlso
    <https://www.nordpoolgroup.com/trading/Day-ahead-trading/Order-types/Block-bid/> ,
    <https://www.epexspot.com/en/tradingproducts> ;
  rdfs:subClassOf
    ieso:Bid ,
    ieso:Constraint ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasEndPeriod ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ieso:Period ;
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasOffer ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ieso:Offer
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasStartPeriod ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ieso:Period ;
    ]  ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:placedInPeriod ;
      owl:someValuesFrom ieso:Period ;
    ] ;
  .
# !



###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/FlexibleOrder
ieso:FlexibleOrder
  rdf:type owl:Class ;
  rdfs:comment "The 'Flexible order' class describes bids to sell flexibility.\n\nThis type of bid is available at Nord Pool Spot market. It is similar to Block orders, however, without the 'minimum number of consecutive periods per block order' restriction."@en ;
  rdfs:label "Flexible order"@en ;
  rdfs:isDefinedBy <trading/v1.0.0/> ;
  rdfs:seeAlso
    <https://www.nordpoolgroup.com/faq-listing/faq/flexible-orders/> ,
    <https://www.nordpoolgroup.com/trading/Day-ahead-trading/Order-types/Flexi-order/> ;
  rdfs:subClassOf
    ieso:BlockOrder ,
    ieso:SupplyBid ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:placedInSession ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ieso:Session
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:flexibleOrderDuration ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:duration
    ] ;
  .

ieso:WindTechnology
  rdf:type owl:Class ;
  rdfs:comment "Nuclear technology produces electric energy from wind power."@en ;
  rdfs:label "Wind technology"@en ;
  rdfs:isDefinedBy <power-transmission-distribution/v1.0.0/> ;

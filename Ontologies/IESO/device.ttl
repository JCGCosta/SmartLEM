  rdfs:label "has command"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:Function ;
  rdfs:range ieso:Command ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/hasDevice
ieso:hasDevice
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The 'has device' object property relates a class or individual with its device(s)."@en ;
  rdfs:label "has device"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:range ieso:Device ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/hasFunction
ieso:hasFunction

###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/ToggleCommand
ieso:ToggleCommand
  rdf:type owl:Class ;
  rdfs:comment "This class defines a toggle command. It is a key or command that is operated the same way but with opposite effect on successive occasions."@en ;
  rdfs:label "Toggle command" ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:subClassOf ieso:Command ;
  owl:equivalentClass [
    rdf:type owl:Class ;
    owl:unionOf (
      [
        owl:intersectionOf ( ieso:CloseCommand ieso:OpenCommand ) ;
        rdf:type owl:Class
      ]
      [
        owl:intersectionOf ( ieso:OffCommand ieso:OnCommand ) ;
        rdf:type owl:Class
      ]
      [
        owl:intersectionOf ( ieso:StartCommand ieso:StopCommand ) ;
        rdf:type owl:Class
      ]
    )
  ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Device
ieso:Device
  rdf:type owl:Class ;
  rdfs:comment "Devices are objects or systems that have a specific purpose or intention. This class abstracts the devices involved in the MAS-Society project."@en ;
  rdfs:label "Device"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Device> ;
  rdfs:subClassOf
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:isComposedBy ;
      owl:someValuesFrom ieso:Device
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:composes ;
      owl:someValuesFrom ieso:Device
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasFunction ;
      owl:someValuesFrom ieso:Function
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasState ;
      owl:someValuesFrom ieso:State
    ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/AirConditioner
ieso:AirConditioner
  rdf:type owl:Class ;
  rdfs:comment "An air conditioner is a system or a machine that treats air in a defined, usually enclosed area via a refrigeration cycle in which warm air is removed and replaced with cooler air."@en ;
  rdfs:label "Air conditioner"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:seeAlso <https://simple.wikipedia.org/wiki/Air_conditioner> ;
  rdfs:subClassOf ieso:Device ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/EnergyStorageSystem
ieso:EnergyStorageSystem
  rdf:type owl:Class ;
  rdfs:comment "Energy storage is the capture of energy produced at one time for use at a later time to reduce imbalances between energy demand and energy production. A device that stores energy is generally called an accumulator or battery."@en ;
  rdfs:label "Energy storage system"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Energy_storage> ;
  rdfs:subClassOf ieso:Device ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Accumulator
ieso:Accumulator
  rdf:type owl:Class ;
  rdfs:comment "An accumulator is an energy storage device: a device which accepts energy, stores energy, and releases energy as needed."@en ;
  rdfs:label "Accumulator"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Accumulator_(energy)> ;
  rdfs:subClassOf ieso:EnergyStorageSystem ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/ElectricBattery
ieso:ElectricBattery
  rdf:type owl:Class ;

#################################################################
#  General axioms
#################################################################


###  owl:Thing Level
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
  ieso:Command
  ieso:Device
  ieso:Function
  ieso:State
  )
] .
# !


###  ieso:Command Level
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
    ieso:GetCommand
    ieso:NotifyCommand
    ieso:PauseCommand
    ieso:SetLevelCommand
    ieso:ToggleCommand
  )
] .


###  ieso:SetLevelCommand Level
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
    ieso:SetAbsoluteLevelCommand
    ieso:SetRelativeLevelCommand
  )
] .
# !


###  ieso:SetAbsoluteLevelCommand Level
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
    ieso:StepDownCommand
    ieso:StepUpCommand
  )
] .
# !


###  ieso:Function Level
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
    ieso:ActingFunction
    ieso:EventFunction
    ieso:ReadingFunction
    ieso:SensingFunction
  )
] .
# !


###  ieso:ActingFunction Level
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
    ieso:CloseFunction
    ieso:LevelControlFunction
    ieso:OffFunction
    ieso:OnFunction
    ieso:OpenFunction
    ieso:PauseFunction
    ieso:StartFunction
    ieso:StopFunction
  )
] .
# !


###  ieso:State Level
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
    ieso:ClosedState
    ieso:MultiLevelState
    ieso:OffState
    ieso:OnState
    ieso:OpenState
    ieso:PauseState
    ieso:StandbyState
    ieso:StartState
    ieso:StopState
  )
] .
# !
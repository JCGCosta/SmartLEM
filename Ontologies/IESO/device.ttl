###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/inRange
ieso:inRange
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The 'in range' object property relates a 'MultiLevelState' class with a list, bag, or sequence of the allowed values"@en ;
  rdfs:label "in range"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:MultiLevelState ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/isCommandOf
ieso:isCommandOf
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The 'is command of' object property relates a command with a device function."@en ;
  rdfs:label "is command of"@en ;
  rdfs:domain ieso:Command ;
  rdfs:range ieso:Function ;
  owl:inverseOf ieso:hasCommand ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/isComposedBy
ieso:isComposedBy
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The 'composed by' object property relates a 'composed' device with the devices that compose it."@en ;
  rdfs:label "is composed by"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:Device ;
  rdfs:range ieso:Device ;
  owl:inverseOf ieso:composes ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/isDeviceOf
ieso:isDeviceOf
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The 'is device of' object property relates a device with a class or individual."@en ;
  rdfs:label "is device of"@en ;
  rdfs:domain ieso:Device ;
  owl:inverseOf ieso:hasDevice ;
  .
# !



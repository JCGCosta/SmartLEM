###  Device Ontology
<device/v1.0.0/>
  rdf:type owl:Ontology ;
  owl:versionIRI <device/v1.0.0/> ;
  owl:versionInfo "v1.0.0" ;
  #owl:priorVersion <device/v1.0.0/> ;
  rdfs:comment "This ontology describes concepts, relations, and properties identified in MAS-Society use cases."@en ;
  #rdfs:isDefinedBy <any-online-specification-document> ;
  #rdfs:seeAlso <uri-to-other-document-or-image> ;
  dcterms:title "GECAD's IESO Device Ontology"@en ;
  dcterms:description "The GECAD's IESO Device Ontology describes devices and respective functions, commands, and states. The GECAD's IESO Measure Ontology <https://www.gecad.isep.ipp.pt/ieso/measure/v1.0.0/> allows the description of device measurements, measurement values, type of measures, and units of measure."@en ;
  dcterms:issued "2019-07-01"^^xsd:date ;
  dcterms:modified "2023-02-21"^^xsd:date ;
  dcterms:publisher <https://www.gecad.isep.ipp.pt/> ;
  dcterms:license <https://www.gecad.isep.ipp.pt/ieso/license/> ;
  dcterms:creator <https://orcid.org/0000-0001-8839-8807> ;
  #dcterms:contributor [ a foaf:Person ; foaf:name "Gabriel Santos" ] ;
  dcterms:source <https://github.com/gabrieljlsantos/gecad-ieso> ;
  vann:preferredNamespacePrefix "ieso" ;
  vann:preferredNamespaceUri ieso: ;
  .
# !



  rdfs:comment "The 'has function' object property relates a device with its functions."@en ;
  rdfs:label "has function"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:Device ;
  rdfs:range ieso:Function ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/hasState
ieso:hasState
  rdf:type owl:ObjectProperty ;
  rdfs:comment "The 'has state' object property relates a device with its state(s)."@en ;
  rdfs:label "has state"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:Device ;
  rdfs:range ieso:State ;
  .

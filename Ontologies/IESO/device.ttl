###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Command
ieso:Command
  rdf:type owl:Class ;
  rdfs:comment "Commands are instructions sent to the devices to perform specific functions that usualy update the device's state."@en ;
  rdfs:label "Command"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:subClassOf
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:actsOn ;
      owl:someValuesFrom ieso:State
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:isCommandOf ;
      owl:someValuesFrom ieso:Function
    ] ;
  .
# !



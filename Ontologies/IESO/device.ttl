###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/maximumLevel
ieso:maximumLevel
  rdf:type
    owl:DatatypeProperty ,
    owl:FunctionalProperty ;
  rdfs:comment "The 'maximum level' datatype property relates a 'MultiLevelState' class with the maximum value allowed"@en ;
  rdfs:label "maximum level"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:MultiLevelState ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/minimumLevel
ieso:minimumLevel
  rdf:type
    owl:DatatypeProperty ,
    owl:FunctionalProperty ;
  rdfs:comment "The 'minimum level' datatype property relates a 'MultiLevelState' class with the minimum value allowed"@en ;
  rdfs:label "minimum level"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:MultiLevelState ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/stateStatus
ieso:stateStatus
  rdf:type
    owl:DatatypeProperty ,
    owl:FunctionalProperty ;
  rdfs:comment "The 'state status' datatype property relates a 'State' class with its current status"@en ;
  rdfs:label "state status"@en ;
  rdfs:isDefinedBy <device/v1.0.0/> ;
  rdfs:domain ieso:State ;
  .
# !



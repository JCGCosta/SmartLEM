  rdfs:label "Demand response contract"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:subClassOf
    ieso:BilateralContract ,
    # FIXME: [entidades de balanço - players dão conhecimento a estas entidades, mas estas não fazem parte da negociação] , <<< ESTA INFO DEVE ESTAR NO CONTRATO? IMAGINO QUE SEJA DA RESPONSABILIDADE DO AGREGADOR...
    # FIXME: [remuneração da quantidade de energia - preço * quantidade de energia] , <<< ACHO QUE FAZ PARTE DA FATURA/RECIBO NO FINAL DO EVENTO/MES, POIS O PREÇO VARIA CONSUANTE O PREÇO DE MERCADO P.EX.
    [
      rdf:type owl:Restriction ;

###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/DRControllableAppliances
ieso:DRControllableAppliances
  rdf:type owl:Class ;
  rdfs:comment "This class describes a contract term promise for automatic control of demand response appliances."@en ;
  rdfs:label "DR controllable appliances"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:subClassOf
    ieso:Promise ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasControllableDevice ;
      owl:someValuesFrom ieso:Device ;
    ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/DREventAdvanceNotice
ieso:DREventAdvanceNotice
  rdf:type owl:Class ;
  rdfs:comment "This class describes a contract term promise for an advance notification time before a demand response event."@en ;
  rdfs:label "DR event advance notice"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:subClassOf
    ieso:Promise ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:advanceNotificationTime ;
      owl:someValuesFrom time:Duration ;
    ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/DRNonControllableAppliances
ieso:DRNonControllableAppliances
  rdf:type owl:Class ;
  rdfs:comment "This class describes a contract term promise for manual or semiautomatic control of demand response appliances, i.e., these appliances are not controlled by the aggregator."@en ;
  rdfs:label "DR non controllable appliances"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:subClassOf
    ieso:Promise ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasNonControllableDevice ;
      owl:someValuesFrom ieso:Device ;
    ] ;
  .
# !



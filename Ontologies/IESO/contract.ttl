###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Grantee
ieso:Grantee
  rdf:type owl:Class ;
  rdfs:comment "A grantee is the recipient of an asset. "@en ;
  rdfs:label "Grantee"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:seeAlso <https://www.investopedia.com/terms/g/grantee.asp> ;
  rdfs:subClassOf
    ieso:Role ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasObligation ;
      owl:someValuesFrom ieso:Promise ; #Obligation
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasRight ;
      owl:someValuesFrom ieso:Promise ; #Right
    ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Grantor
ieso:Grantor
  rdf:type owl:Class ;
  rdfs:comment "A Grantor is a person or entity that conveys ownership of an asset to another person or entity: the grantee."@en ;
  rdfs:label "Grantor"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:seeAlso <https://www.investopedia.com/terms/g/grantor.asp> ;
  rdfs:subClassOf
    ieso:Role ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasObligation ;
      owl:someValuesFrom ieso:Promise ; #Obligation
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasRight ;
      owl:someValuesFrom ieso:Promise ; #Right
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:provides ;
      owl:someValuesFrom [
        rdf:Type owl:Class ;
        owl:unionOf (
          ieso:Good
          ieso:Service
        )
      ]
    ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Service
ieso:Service
  rdf:type owl:Class ;
  rdfs:comment "This class abstracts goods to be traded within contracts.\n\nA service is a transaction in which no physical goods are transferred from the seller to the buyer. The benefits of such a service are held to be demonstrated by the buyer's willingness to make the exchange. Services are intangible in nature, and may be defined as acts or performances whereby the service provider provides value to the customer."@en ;
  rdfs:label "Service"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Service_(economics)> ;
  .
# !



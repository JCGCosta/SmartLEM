###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Penalty
ieso:Penalty
  rdf:type owl:Class ;
  rdfs:comment "This abstract class describes a contract term penalty."@en ;
  rdfs:label "Penalty"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/ExclusionPenalty
ieso:ExclusionPenalty
  rdf:type owl:Class ;
  rdfs:comment "This class describes an exclusion penalty."@en ;
  rdfs:label "Exclusion penalty"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:subClassOf
    ieso:Penalty ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:exclusionDuration ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:duration
    ] ;

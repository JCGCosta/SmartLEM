###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/MonetaryPenalty
ieso:MonetaryPenalty
  rdf:type owl:Class ;
  rdfs:comment "This class describes a monetary penalty."@en ;
  rdfs:label "Monetary penalty"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:subClassOf
    ieso:Penalty ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasPrice ;

###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/Contract
ieso:Contract
  rdf:type owl:Class ;
  rdfs:comment "The Contract class abstracts contracts in the scope of MAS-Society project use cases.\n\nA contract is a legally binding document between at least two parties that defines and governs the rights and duties of the parties to an agreement. A contract is legally enforceable because it meets the requirements and approval of the law. A contract typically involves the exchange of goods, services, money, or promise of any of those."@en ;
  rdfs:label "Contract"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Contract> ;
  rdfs:subClassOf
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:executionStartsAt ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass time:TemporalEntity
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:executionExpiresAt ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass time:TemporalEntity
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasGood ;
      owl:someValuesFrom ieso:Good
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasGrantor ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ieso:Grantor
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasRemuneration ;
      owl:someValuesFrom ieso:Price
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasService ;
      owl:someValuesFrom ieso:Service
    ] , [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasTerm ;
      owl:someValuesFrom ieso:ContractualTerm
    ] ;
  .
# !


###  https://www.gecad.isep.ipp.pt/ieso/v1.0.0/BilateralContract
ieso:BilateralContract
  rdf:type owl:Class ;
  rdfs:comment "A Bilateral contract is an agreement between two parties in which each of the parties to the contract makes a promise or set of promises to each other to purchase/provide a commodity."@en ;
  rdfs:label "Bilateral contract"@en ;
  rdfs:isDefinedBy <contract/v1.0.0/> ;
  rdfs:seeAlso
    <http://www.mascem.gecad.isep.ipp.pt/ontologies/electricity-markets.owl#BilateralContract> ,
    <https://www.investopedia.com/terms/b/bilateral-contract.asp> ,
    <https://en.wikipedia.org/wiki/Contract#Offer_and_acceptance> ;
  rdfs:subClassOf
    ieso:Contract ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty ieso:hasGrantee ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass ieso:Grantee
    ] ;
  .
# !


